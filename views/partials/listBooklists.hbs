<div class="ui centered cards">
  {{#each booklists}}
    <div class="ui card">
      <div class="image">
        <img src="{{picture}}" />
      </div>
      <div class="content">
        <a class="header">{{name}}</a>
      </div>
      <div class="extra content">
        <a href="/details/{{id}}">Details</a>
        <a href="/dashboard/deletebooklist/{{id}}" class="ui icon button delbooklist">
          <i class="ui trash icon"></i>
        </a>
      </div>
    </div>
  {{/each}}
</div>